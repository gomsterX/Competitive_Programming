#Problem ID: SWPDGT
#Problem Name: Chef Swaps Digits

for _ in range(int(input())):
    s1 = list(input())
    s2 = list(input())

    for i in range(len(s1)):
        for j in range(len(s2)):
            if(s1[i] < s2[j] and (i < j or (i == j and len(s1)<len(s2))))
                    s1[i], s2[j] = s2[j], s1[j]
            elif(s1[i] > s2[j] and (i > j or (i == j and len(s1)<len(s2))))
                    s1[i], s2[j] = s2[j], s1[j]
    print(s1)
    print(s2)
